{
  "name": "algorythmos-ai-agents",
  "version": "1.0.0",
  "description": "AI-Powered Workflow Automation Platform",
  "private": true,
  "keywords": [
    "workflow",
    "automation",
    "ai",
    "python",
    "fastapi",
    "nextjs",
    "celery",
    "postgresql",
    "redis",
    "kubernetes"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/algorythmos-ai-agents.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/algorythmos-ai-agents/issues"
  },
  "homepage": "https://github.com/your-username/algorythmos-ai-agents#readme",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "start:python": "./start-python.sh",
    "start:k8s": "./k8s/deploy.sh",
    "setup:k8s": "./k8s/kind-setup.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "db:migrate": "cd apps/api-python && alembic upgrade head",
    "db:generate": "cd apps/api-python && alembic revision --autogenerate",
    "worker:start": "cd apps/worker-python && celery -A celery_app worker --loglevel=info",
    "worker:beat": "cd apps/worker-python && celery -A celery_app beat --loglevel=info",
    "api:start": "cd apps/api-python && uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "web:start": "cd apps/web && npm run dev",
    "install:all": "npm install && cd apps/web && npm install",
    "setup:env": "cp env.python.example .env",
    "check:env": "node -e \"require('dotenv').config(); console.log('Environment check:', process.env.OPENAI_API_KEY ? 'OK' : 'Missing API keys')\""
  },
  "devDependencies": {
    "@types/node": "^20.19.13",
    "tsx": "^4.0.0",
    "turbo": "^1.10.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4",
    "dotenv": "^16.0.0"
  },
  "workspaces": [
    "apps/*"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  }
}
